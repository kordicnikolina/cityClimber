                            <div class="col-lg-6 stand-img-des">
                                <div class="d-inline-block">
                                   
                                    <h2 class="heading-text" style="margin-bottom:80px">${uslugeList[i].naziv}</h2>
                                    <div class="para_text">
                                    <p style="text-align:justify;line-height:1.8">${uslugeList[i].opis} </p></div>
                                </div>
                            </div>
                            
                                <div class="col-lg-6">
                                <div class="blog-image wow hover-effect fadeInLeft text-center text-lg-left" style="margin-left:50px">
