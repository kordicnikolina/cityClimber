                                <div class="blog-image wow hover-effect fadeInLeft text-center text-lg-left">
                                    <img src="${uslugeList[i].slika }" alt="image">
                                </div>
                            </div>
                           
                            <div class="col-lg-6 stand-img-des">
                                <div class="d-inline-block">
                                   
                                    <h2 class="heading-text" style="margin-bottom:80px">${uslugeList[i].naziv}</h2>
                                    <p class="para_text" style="text-align:justify">${uslugeList[i].opis} </p>
                                </div>
                            </div>
                            
                        </div>
                        
                        </c:if>
                        <c:if test="${i%2==1}">
